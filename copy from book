#include<bits/stdc++.h>
using namespace std;
void quicksort(int left, int right ,int *a)
{
	int temp,i,j;
	if (left > right)
		return;
	temp = a[left];
	i = left;
	j = right;
	while (i != j)
	{
		while (a[j] >= temp && i < j)
			j--;
		while (a[i] <= temp && i < j)
			i++;
		if (i < j)
		{
			int x = a[j];
			a[j] = a[i];
			a[i] = x;
		}
	}
	a[left] = a[i];
	a[i] = temp;
	quicksort(left, i - 1, a);
	quicksort(i + 1, right, a);
}
int main()
{
	int a[100], i;	
	srand((int)time(0));
	for (i = 0; i <= 99; i++)
	{
		a[i]=rand()%1000;
	}
	quicksort(0, 99, a);
	int k=0;
	for (i = 0; i <= 99; i++)
	{
		cout << a[i] <<" ";
		k++;
		if (k % 10 == 0)
			cout << endl;
	}

	return 0;
}
